//每次循环找到剩余数组元素中的最小值

public class SeLection {
	public SeLection(Comparable[] a) {
		for (int i = 0; i < a.length; i++) {
			int min = i;
			for (int j = i+1; j < a.length; j++) {
				if(a[j].compareTo(a[min])<0) {
					min = j;
				}
				Comparable temp = a[i];
				a[i] = a[min];
				a[j] = temp;
			}
		}
	}
}
